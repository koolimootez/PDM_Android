<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="M3.AppTheme" parent="Theme.Material3.Dark.NoActionBar">
        <item name="colorPrimary">@color/material_on_background_emphasis_medium</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorPrimaryContainer">@color/material_dynamic_primary70</item>
        <item name="colorOnPrimaryContainer">@color/white</item>

        <item name="colorSecondary">@color/black</item>
        <item name="colorOnSecondary">@color/design_default_color_error</item>
        <item name="colorSecondaryContainer">@color/material_on_surface_stroke</item>
        <item name="colorOnSecondaryContainer">@color/white</item>

        <item name="colorTertiary">@color/design_default_color_secondary</item>
        <item name="colorOnTertiary">@color/cardview_dark_background</item>
        <item name="colorTertiaryContainer">@color/material_dynamic_neutral60</item>
        <item name="colorOnTertiaryContainer">@color/material_dynamic_neutral10</item>

        <item name="colorError">#B32E</item>
        <item name="colorErrorContainer">#B3771E</item>
        <item name="colorOnError">#B3451E</item>
        <item name="colorOnErrorContainer">#B3961E</item>

        <item name="android:colorBackground">@color/material_dynamic_neutral_variant40</item>
        <item name="colorOnBackground">@color/cardview_light_background</item>

        <item name="colorSurface">@color/material_dynamic_tertiary99</item>
        <item name="colorOnSurface">@color/white</item>
        <item name="colorSurfaceVariant">@color/design_default_color_primary_dark</item>
        <item name="colorOnSurfaceVariant">@color/white</item>

        <item name="colorOutline">@color/white</item>
        <item name="colorOnSurfaceInverse">@color/white</item>
        <item name="colorSurfaceInverse">@color/material_on_primary_emphasis_medium</item>
        <item name="colorPrimaryInverse">@color/material_dynamic_neutral70</item>

        <item name="android:statusBarColor">@color/material_dynamic_neutral0</item>
        <item name="android:textColorHighlight">@color/design_default_color_background</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>

    </style>


</resources>